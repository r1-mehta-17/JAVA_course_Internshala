<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="rootGridPane" style="-fx-background-color: #D9F7F0;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.game.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints maxWidth="301.0" minWidth="217.0" prefWidth="217.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="25.0" />
      <RowConstraints />
   </rowConstraints>
   <Pane GridPane.columnSpan="2" />
   <Pane fx:id="insertedDiscPane" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="1" />
   <VBox style="-fx-background-color: #2B3B4C;" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <TextField fx:id="playerOneTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="188.0" promptText="Player One Name">
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="35.0" />
         </VBox.margin></TextField>
      <TextField fx:id="playerTwoTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="188.0" promptText="Player Two Name">
         <VBox.margin>
            <Insets left="15.0" right="15.0" />
         </VBox.margin></TextField>
      <Button fx:id="setNamesButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="188.0" text="Set Names">
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin></Button>
      <Label fx:id="playerNameLabel" alignment="CENTER" prefHeight="43.0" prefWidth="304.0" text="PLAYER ONE" textFill="WHITE">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
         <VBox.margin>
            <Insets top="70.0" />
         </VBox.margin>
      </Label>
      <Label alignment="CENTER" prefHeight="38.0" prefWidth="301.0" text="TURN" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </VBox>
</GridPane>
